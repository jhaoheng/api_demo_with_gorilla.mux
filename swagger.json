    "/csrf": {
      "get": {
        "description": "To operate the apis, get CSRF token at beginning\n```\ncurl -X GET 'https://localhost/csrf' --insecure\n```",
        "operationId": "GetCSRFToken",
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "string"
                },
                "error": {
                  "type": "string"
                }
              },
              "example": {
                "data": "success",
                "error": ""
              }
            },
            "headers": {
              "X-CSRF-Token": {
                "description": "CSRF Token"
              }
            }
          }
        }
      }
    },
    "/signin": {
      "post": {
        "description": "will return jwt token\n```\ncurl -X POST 'https://localhost/signin' \\\n-H 'X-CSRF-Token: {{X-CSRF-Token}}' \\\n-H 'Content-Type: application/json' \\\n-d '{\n\"account\": \"\",\n\"password\": \"\"\n}' \\\n--insecure\n```",
        "tags": [
          "auth"
        ],
        "summary": "signin",
        "operationId": "SignIn",
        "parameters": [
          {
            "type": "string",
            "name": "X-CSRF-Token",
            "in": "header",
            "required": true
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "required": [
                "account",
                "password"
              ],
              "properties": {
                "account": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "object"
                },
                "error": {
                  "type": "string"
                }
              },
              "example": {
                "data": {
                  "token": "..."
                },
                "error": ""
              }
            }
          },
          "400": {
            "description": "something wrong",
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "nullable": true
                },
                "error": {
                  "type": "string"
                }
              },
              "example": {
                "data": null,
                "error": "the error detail..."
              }
            }
          }
        }
      }
    },
